function showData() {
  $('#wishWrapper').html('');
  $.ajax({
    type: 'get',
    url: '/listWish',
    success: function (response) {
      let stringResult = '';
      response.forEach((item, index) => {
        if (index % 2 == 0) {
          stringResult += `<div class="wish-box-item">
															<strong>${item.name}</strong>
															<p>${item.content}</p>
														</div>`;
        } else {
          stringResult += `<div class="wish-box-item bg">
															<strong>${item.name}</strong>
															<p>${item.content}</p>
														</div>`;
        }
      });
      $('#wishWrapper').html(stringResult);
    },
  });
}
